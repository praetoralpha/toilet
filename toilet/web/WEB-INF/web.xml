<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
    <context-param>
        <param-name>ApplicationPrimer.url</param-name>
        <param-value>http://[::1]:8080/toilet</param-value>
    </context-param>
    <context-param>
        <param-name>ApplicationPrimer.ua</param-name>
        <param-value>toilet primer</param-value>
    </context-param>
    <context-param>
        <param-name>ApplicationPrimer.timeout</param-name>
        <param-value>0</param-value>
    </context-param>
    <absolute-ordering>
        <name>libOdyssey</name>
        <name>libIMEAD</name>
        <name>libHtmlTools</name>
        <others/>
    </absolute-ordering>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <scripting-invalid>true</scripting-invalid>
            <trim-directive-whitespaces>true</trim-directive-whitespaces>
        </jsp-property-group>
    </jsp-config>
    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>
    <error-page>
        <error-code>400</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <error-code>401</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <error-code>404</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <error-code>405</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <error-code>501</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <error-code>599</error-code>
        <location>/coroner</location>
    </error-page>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/coroner</location>
    </error-page>
</web-app>
