<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd">
  <tlib-version>1.0</tlib-version>
  <short-name>ody</short-name>
  <uri>uri:libwebsitetools:Odyssey</uri>
  <tag>
    <name>responseTag</name>
    <description>Sets two variables inside: requestTime (when the request first arrived) and renderMillis (number of milliseconds since requestTime).</description>
    <tag-class>libOdyssey.ResponseTag</tag-class>
    <body-content>scriptless</body-content>
  </tag>
</taglib>
